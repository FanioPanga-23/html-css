	' 	single quotation mark 	&apos; 	&#39; 	
	¢ 	cent 	&cent; 		&#162; 	
	£ 	pound 	&pound; 	&#163; 	
	¥ 	yen 	&yen; 		&#165; 	
	€ 	euro 	&euro; 		&#8364; 	
	© 	copyright 		&copy; 	&#169; 	
	® 	trademark 		&reg; 	&#174;
